{
  "name": "chatwork info maker",
  "version": "0.9.3",
  "manifest_version": 2,
  "description": "今見ているページから、chatworkのinfo形式文字列を作るアドオンです",
  "homepage_url": "https://github.com/okisanjp/chrome-ex-chatwork-info",
  "icons": {
    "16" : "images/16.png",
    "48" : "images/48.png",
    "128" : "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/16.png",
    "default_title": "chatwork info maker",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
